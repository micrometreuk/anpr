server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;
	index index.html index.htm index.nginx-debian.html;

	server_name localhsot;

	location / {
        auth_basic "Private Property";
        auth_basic_user_file /etc/nginx/.htpasswd;
      	proxy_pass http://localhost:9091/;
	}

}
