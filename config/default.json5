// Json5 format, see https://json5.org/
{
    "config": {
        "listeningPort": 9091,
        "pageTitle": "Linux Webmin",
        "links": [
            {
                "label": "GitHub",
                "url": "https://github.com/micrometreuk",
                "newTab": true
            }
        ],

        "commands": [
	    {
                "label": "host name",
                "nickname": "host",
                "exec": "hostname"
            },            
	    {
                "label": "host uptime",
                "nickname": "uptime",
                "exec": "uptime"
            },
            {
                "label": "usernames and numbers ",
                "nickname": "usersnmubers",
                "exec": "cat /etc/passwd"
            },
            {
                "label": "Public IP",
                "nickname": "ip1",
                "exec": "curl -s http://checkip.dyndns.org/ | sed 's/[a-zA-Z<>/ :]//g'"
            },
            {
                "label": "linux version",
                "nickname": "uname",
                "exec": "uname -r"
            },
            {
                "label": "linux distribution",
                "nickname": "osrelease",
                "exec": "cat /etc/os-release"
            },
	    {
                "label": "installedlled packages",
                "nickname": "aptlist",
                "exec": "apt list --installed"
            },
            {
                "label": "top 5 process",
                "nickname": "psaux",
                "exec": "ps -eo pid,comm,%cpu,%mem --sort=-%cpu | head -n 5"
            },
	    {
                "label": "List mount points",
                "nickname": "mounts",
                "exec": "mount"
            }
        ],

        "informations": [
	    {
                "label": "motd",
                "exec": "ls -la"
            }
        ]
    }
}
